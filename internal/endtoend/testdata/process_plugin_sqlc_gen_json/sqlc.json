{
  "version": "2",
  "sql": [
    {
      "schema": "schema.sql",
      "queries": "query.sql",
      "engine": "postgresql",
      "codegen": [
        {
          "out": "gen",
          "plugin": "jsonb",
          "options": {
            "indent": "  ",
            "filename": "codegen.json"
          }
        }
      ]
    }
  ],
  "plugins": [
    {
      "name": "jsonb",
      "process": {
        "cmd": "go run ../../cmd/sqlc-gen-json"
      }
    }
  ]
}
